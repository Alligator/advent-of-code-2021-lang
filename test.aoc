file: 'input.txt'

part1: {
  for line1 in lines {
    for line2 in lines {
      var sum = num(line1) + num(line2)
      if sum == 2020 {
        return num(line1) * num(line2)
      }
    }
  }
}

part2: {
  for line1 in lines {
    for line2 in lines {
      for line3 in lines {
        var sum = num(line1) + num(line2) + num(line3)
        if sum == 2020 {
          return num(line1) * num(line2) * num(line3)
        }
      }
    }
  }
}